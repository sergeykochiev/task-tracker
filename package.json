{
  "name": "discord-task-tracker-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Discord task tracker bot",
  "main": "src/index.ts",
  "scripts": {
    "start": "npx tsx src/index.ts",
    "run": "npx tsx",
    "cmds": "npx tsx script/commands.ts",
    "macros": "npx tsx ./script/add-default-macros.ts",
    "dev": "npx nodemon src/index.ts",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "shmsync": "npm run typeorm schema:sync --  -d src/db/data-source.ts",
    "mgen": "npm run typeorm migration:generate -- src/db/migration/$npm_config_name -d src/db/data-source.ts",
    "mrun": "npm run typeorm migration:run -- -d src/db/data-source.ts",
    "mrevert": "npm run typeorm migration:revert -- -d src/db/data-source.ts"
  },
  "author": "Sergey Kochiev",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "discord-interactions": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@octokit/types": "^13.6.1",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/ws": "^8.5.12",
    "discord-api-types": "^0.37.101",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
