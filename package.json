{
  "name": "discord-task-tracker-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Discord task tracker bot",
  "main": "app.ts",
  "scripts": {
    "start": "node api/app.ts",
    "run": "npx tsx",
    "register": "npx tsx script/commands.ts",
    "dev": "npx nodemon api/app.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npm run typeorm schema:sync --  -d src/db/data-source.ts",
    "migration:gen": "npm run typeorm migration:generate -- src/db/migration/%npm_config_name% -d src/db/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/db/data-source.ts"
  },
  "author": "Sergey Kochiev",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "discord-interactions": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@octokit/types": "^13.6.1",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/ws": "^8.5.12",
    "discord-api-types": "^0.37.101",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
